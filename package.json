{
  "name": "akurat",
  "version": "0.1.0",
  "bin": {
    "akurat": "bin/akurat.js"
  },
  "scripts": {
    "install_jwt_verifier_layer": "cd layers/jwt-verifier/nodejs/node_modules/jwt-verifier && npm install",
    "install_lambda_layers": "npm run install_jwt_verifier_layer",
    "postinstall": "(npm run install_lambda_layers);",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --testPathPattern=test/",
    "test:int": "jest --testPathPattern=int/ --runInBand",
    "cdk": "cdk"
  },
  "dependencies": {
    "aws-cdk-lib": "2.93.0",
    "constructs": "10.2.70",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.405.0",
    "@aws-sdk/client-dynamodb": "3.405.0",
    "@aws-sdk/util-dynamodb": "3.405.0",
    "aws-jwt-verify": "4.0.0",
    "@types/jest": "29.5.4",
    "@types/node": "18.17.12",
    "@types/supertest": "2.0.12",
    "aws-sdk": "2.1447.0",
    "jest": "29.6.4",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "aws-cdk": "2.93.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
